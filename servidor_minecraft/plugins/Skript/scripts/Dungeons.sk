command /dleave:
	aliases: /dungeonleave
	trigger:
		if player's world is "world":
			make player execute "warp dungeons"
		else:
			send "&f &fYou can't do that here!" to player

command /ddebug [<text>]:
	permission: op
	trigger:
		if arg-1 is "arena1":
			send "&f &fMonsters from the &e%arg-1%&f have been set to 0." to player
			set {arena1mobs} to 0
		else if arg-1 is "arena2":
			send "&f &fMonsters from the &e%arg-1%&f have been set to 0." to player
			set {arena2mobs} to 0
		else if arg-1 is "arena2g":
			send "&f &fMonsters from the &e%arg-1%&f have been set to 0." to player
			set {arena2mobsg} to 0
		else if arg-1 is "arena3":
			send "&f &fMonsters from the &e%arg-1%&f have been set to 0." to player
			set {arena3mobs} to 0
		else if arg-1 is "arena3g":
			send "&f &fMonsters from the &e%arg-1%&f have been set to 0." to player
			set {arena3mobsg} to 0
		else:
			send "&f &fThe specified arena does not exist!" to player

