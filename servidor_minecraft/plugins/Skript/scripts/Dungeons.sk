command /dhelp:
	permission: op
	trigger:
		send "&8 »   &e▪&f▪&6▪ &fDungeons - Admin &6▪&f▪&e▪ &8   « " to player
		send "&e/dleave &8- &fUse this to get out of an arena" to player
		send "&e/dkey <player> <arena> &8- &fGive a entry key to a player" to player
		send "&e/ddebug <arena> &8- &fUse it when the monsters aren't spawned in an arena" to player
		send "&e/dmobs <arena> &8- &fShow the theoretical number of monsters in the arena" to player
		send "&e/dloc <add/remove> <arena> <number> &8- &fUpdate monster's spawn location" to player
		send "&8Available arenas for /dkey: &7easy, medium, hard"
		send "&8Available arenas for the others: &7arena1, arena2, arena2g, arena3, arena3g"
		send "&8 »   &e▪&f▪&6▪ &fDungeons - Admin &6▪&f▪&e▪ &8   « " to player

command /dkey [<player>] [<text>]:
	permission: op
	trigger:
		if arg-1 is not set:
			send "&f &fYou did not specify a player." to player
		else:
			if arg-2 is "easy":
				send "&f &fYou have given player &e%arg-1%&f an easy key!" to player
				set {_item} to stick
				set name of {_item} to "&f"
				set the 1nd line of the lore of {_item} to "&7ɪᴛ ᴄᴀɴ ʙᴇ ᴜꜱᴇᴅ ᴛᴏ ᴇɴᴛᴇʀ"
				set the 2nd line of the lore of {_item} to "&7ᴛʜᴇ ᴇᴀꜱʏ ᴅᴜɴɢᴇᴏɴ ᴀʀᴇɴᴀ"
				set the 4nd line of the lore of {_item} to "&8ɢɪᴠᴇ ᴛʜɪs ᴋᴇʏ ᴛᴏ ᴛʜʏʀᴀ"
				set custom model data of {_item} to 145
				give 1 of {_item} to arg-1
			else if arg-2 is "medium":
				send "&f &fYou have given player &e%arg-1%&f a medium key!" to player
				set {_item} to stick
				set name of {_item} to "&f"
				set the 1nd line of the lore of {_item} to "&7ɪᴛ ᴄᴀɴ ʙᴇ ᴜꜱᴇᴅ ᴛᴏ ᴇɴᴛᴇʀ"
				set the 2nd line of the lore of {_item} to "&7ᴛʜᴇ ᴍᴇᴅɪᴜᴍ ᴅᴜɴɢᴇᴏɴ ᴀʀᴇɴᴀ"
				set the 4nd line of the lore of {_item} to "&8ɢɪᴠᴇ ᴛʜɪs ᴋᴇʏ ᴛᴏ ᴛʜʏʀᴀ"	
				set custom model data of {_item} to 146
				give 1 of {_item} to arg-1
			else if arg-2 is "hard":
				send "&f &fYou have given player &e%arg-1%&f a hard key!" to player
				set {_item} to stick
				set name of {_item} to "&f"
				set the 1nd line of the lore of {_item} to "&7ɪᴛ ᴄᴀɴ ʙᴇ ᴜꜱᴇᴅ ᴛᴏ ᴇɴᴛᴇʀ"
				set the 2nd line of the lore of {_item} to "&7ᴛʜᴇ ʜᴀʀᴅ ᴅᴜɴɢᴇᴏɴ ᴀʀᴇɴᴀ"
				set the 4nd line of the lore of {_item} to "&8ɢɪᴠᴇ ᴛʜɪs ᴋᴇʏ ᴛᴏ ᴛʜʏʀᴀ"	
				set custom model data of {_item} to 147
				give 1 of {_item} to arg-1
			else:
				send "&f &fThe mentioned key is invalid!" to player	


command /opendungeoneasy [<player>]:
	permission: op
	trigger:
		if name of tool of arg-1 is "&f":
			remove 1 of tool of arg-1 from arg-1's inventory
			send "&f &fYou will arrive in the arena immediately! Remember that you can only exit with a teleportation command, such as &e/dleave&f." to arg-1
			wait 2 ticks
			make console execute command "effect give %arg-1% minecraft:nausea 8 5"
			wait 5 seconds
			make console execute command "gms %arg-1%"
			set arg-1's flight mode to false
			make console execute command "god %arg-1% disable"
			teleport arg-1 to {spawnlocarena1}
		else:
			send "&f &fI can't accept the item you're holding to take you to the &eeasy arena&f!" to arg-1
			

command /opendungeoneasymenu [<player>]:
	permission: op
	trigger:
		set {_player} to arg-1 or player
		set {_found} to false
		loop all items in {_player}'s inventory:
			if name of loop-item is "&f":
				remove 1 of loop-item from {_player}'s inventory
				set {_found} to true
				stop loop
		if {_found} is true:
			send "&f &fThe key was checked and the teleportation started! Remember that you can only exit with a teleportation command, such as &e/dleave&f." to arg-1
			wait 2 ticks
			make console execute command "effect give %arg-1% minecraft:nausea 8 5"
			wait 5 seconds
			make console execute command "gms %arg-1%"
			set arg-1's flight mode to false
			make console execute command "god %arg-1% disable"
			teleport arg-1 to {spawnlocarena1}
		else:
			send "&f &fNo key was found in your inventory for &eeasy arena&f!" to arg-1


