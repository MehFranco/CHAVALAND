	permission: op
	trigger:
		if arg-1 is "arena1":
			set {spawnlocarena1} to player's location
			send "&f &fLocation set successfully!" to player
		else if arg-1 is "arena2":
			set {spawnlocarena2} to player's location
			send "&f &fLocation set successfully!" to player
		else if arg-1 is "arena3":
			set {spawnlocarena3} to player's location
			send "&f &fLocation set successfully!" to player
		else:
			send "&f &fThe specified arena is invalid." to player


