command /dloc [<text>] [<text>] [<integer>]:
	permission: op
	trigger:
		if arg-1 is "add":
			if arg-2 is "arena1":
				if arg-3 is not set:
					send "&f &fArgument 3 is invalid. Use &e/dloc (add/remove) (arena1/arena2/arena2g/arena3/arena3g) (number)&f!" to player
				else:
					set {locationarena1::%arg-3%} to player's location
					send "&f &fLocation added successfully!" to player
			else if arg-2 is "arena2":
				if arg-3 is not set:
					send "&f &fArgument 3 is invalid. Use &e/dloc (add/remove) (arena1/arena2/arena2g/arena3/arena3g) (number)&f!" to player
				else:
					set {locationarena2::%arg-3%} to player's location
					send "&f &fLocation added successfully!" to player
			else if arg-2 is "arena2g":
				if arg-3 is not set:
					send "&f &fArgument 3 is invalid. Use &e/dloc (add/remove) (arena1/arena2/arena2g/arena3/arena3g) (number)&f!" to player
				else:
					set {locationarena2g::%arg-3%} to player's location
					send "&f &fLocation added successfully!" to player
			else if arg-2 is "arena3":
				if arg-3 is not set:
					send "&f &fArgument 3 is invalid. Use &e/dloc (add/remove) (arena1/arena2/arena2g/arena3/arena3g) (number)&f!" to player
				else:
					set {locationarena3::%arg-3%} to player's location
					send "&f &fLocation added successfully!" to player
			else if arg-2 is "arena3g":
				if arg-3 is not set:
					send "&f &fArgument 3 is invalid. Use &e/dloc (add/remove) (arena1/arena2/arena2g/arena3/arena3g) (number)&f!" to player
				else:
					set {locationarena3g::%arg-3%} to player's location
					send "&f &fLocation added successfully!" to player
			else:
				send "&f &fArgument 2 is invalid. Use &e/dloc (add/remove) (arena1/arena2/arena2g/arena3/arena3g) (number)&f!" to player
		else if arg-1 is "remove":
			if arg-2 is "arena1":
				if arg-3 is not set:
					send "&f &fArgument 3 is invalid. Use &e/dloc (add/remove) (arena1/arena2/arena2g/arena3/arena3g) (number)&f!" to player
				else:
					delete {locationarena1::%arg-3%} 
					send "&f &fLocation removed successfully!" to player
			else if arg-2 is "arena2":
				if arg-3 is not set:
					send "&f &fArgument 3 is invalid. Use &e/dloc (add/remove) (arena1/arena2/arena2g/arena3/arena3g) (number)&f!" to player
				else:
					delete {locationarena2::%arg-3%} 
